{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/test-connection", "dest": "/api/test-connection" },
    { "src": "/api/contact", "dest": "/api/contact" },
    { "src": "/api/audit-requests", "dest": "/api/audit-requests" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}